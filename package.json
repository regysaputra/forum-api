		"dotenv": "^16.4.1",
		"hapi-auth-bearer-token": "^8.0.0",
		"instances-container": "^2.0.6",
		"nanoid": "^3.3.7",
		"pg": "^8.11.3"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
